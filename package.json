{
  "name": "comment-frame-generator",
  "displayName": "comment-frame-generator",
  "description": "add a frame to an inline comment",
  "version": "0.0.1",
  "publisher": "meow-noisy",
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:comment-frame-generator.generateFrame"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "comment-frame-generator.generateFrame",
        "title": "Generate a comment frame"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "comment-frame-generator.generateFrame",
          "when": "resourceLangId == python",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}